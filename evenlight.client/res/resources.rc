#include "../inc/appinfo.h"

// Icon with lowest ID value placed first to ensure
// the application icon remains consistent on all systems
IDI_APPICON ICON DISCARDABLE "appicon.ico"

1 VERSIONINFO
FILEVERSION     1, 0, 0, 0
PRODUCTVERSION  1, 0, 0, 0
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "CompanyName",        "Arcanum Softworks"
            VALUE "FileDescription",    "Evenlight Client"
            VALUE "FileVersion",        "1.0.0.0"
            VALUE "InternalName",       "Arcanum Engine"
            VALUE "LegalCopyright",     "© 2019 Arcanum Softworks"
            VALUE "OriginalFilename",   "Evenlight.exe"
            VALUE "ProductName",        "Evenlight"
            VALUE "ProductVersion",     "1.0.0.0"
        END
    END
    BLOCK "VarFileInfo"
        BEGIN
        // Translation Values: German / U.S. English
        VALUE "Translation", 0x407, 1252, 0x409, 1252
    END
END
